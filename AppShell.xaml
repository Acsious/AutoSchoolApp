<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="AutoSchoolApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:AutoSchoolApp"
    xmlns:pages="clr-namespace:AutoSchoolApp.Pages"
    Title="AutoSchoolApp"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent  ContentTemplate="{DataTemplate pages:OnboardingPage}"/>
    <ShellContent  Title="HomePage"
                    Route="HomePage"
                    ContentTemplate="{DataTemplate pages:HomePage}"/>

</Shell>
